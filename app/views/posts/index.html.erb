<%= render "shared/header" %>

<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、
        <%= link_to "#{current_user.nickname} さん", root_path(current_user), class: :greeting__link%>
      </div>
    <% end %>
    <div class="card__wrapper">
      <%#= render partial: 'prototype', collection: @prototypes %>
    </div>
  </div>
</main>
